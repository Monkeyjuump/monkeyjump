config_version=5

[application]

config/name="tuwing tuwing"
run/main_scene="res://scenes/title_menu.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

Global="*res://global.gd"

[display]

window/size/viewport_width=180
window/size/viewport_height=320
window/size/window_width_override=360
window/size/window_height_override=640
window/stretch/mode="canvas_items"

[input_devices]

pointing/emulate_touch_from_mouse=true

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
environment/defaults/default_clear_color=Color(0.615686, 0.815686, 0.909804, 1)
